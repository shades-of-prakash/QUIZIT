{
	"name": "bun-react-template",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"homepage": "/",
	"scripts": {
		"dev": "concurrently \"bun  tailwindcss -i ./src/index.css -o ./src/output.css --watch\" \"bun --hot src/index.tsx\"",
		"start": "NODE_ENV=production bun src/index.tsx",
		"build": "bunx tailwindcss -i ./src/index.css -o ./src/output.css  && bun run build.ts"
	},
