		"@types/papaparse": "^5.3.16",
		"@types/prismjs": "^1.26.5",
		"@types/unescape-js": "^1.0.3",
		"bcryptjs": "^3.0.2",
		"bun-plugin-tailwind": "^0.0.15",
		"csv-stringify": "^6.6.0",
		"dompurify": "^3.2.7",
